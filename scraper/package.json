{
  "name": "yad2-scraper",
  "version": "1.0.0",
  "description": "Yad2 listing scraper with Playwright",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "find-location": "node --loader ts-node/esm src/scripts/findLocation.ts",
    "update-locations": "node --loader ts-node/esm src/scripts/updateLocations.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["yad2", "scraper", "playwright"],
  "author": "Ron Levi",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "playwright": "^1.40.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  }
}
